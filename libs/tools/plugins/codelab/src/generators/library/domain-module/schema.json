{
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "id": "CodelabNestDomainModule",
  "title": "Create a Nest Domain Module for Codelab",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "Module name (scaffolds \"libs/modules/[module-name]\")",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "What name would you like to use for this module?"
    }
  },
  "required": ["name"]
}
