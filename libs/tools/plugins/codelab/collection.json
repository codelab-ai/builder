{
  "$schema": "../../../../node_modules/@angular-devkit/schematics/collection-schema.json",
  "name": "react",
  "version": "0.0.1",
  "schematics": {
    "react-lib": {
      "factory": "./src/schematics/library/react/schematic",
      "schema": "./src/schematics/library/react/schema.json",
      "description": "Create a React library"
    },
    "nest-lib": {
      "factory": "./src/schematics/library/nest/schematic",
      "schema": "./src/schematics/library/nest/schema.json",
      "description": "Create a Nest.js library"
    },
    "domain-module": {
      "factory": "./src/schematics/library/domain-module/schematic",
      "schema": "./src/schematics/library/domain-module/schema.json",
      "description": "Create a Nest.js Domain Module"
    },
    "use-case": {
      "factory": "./src/schematics/library/use-case/schematic",
      "schema": "./src/schematics/library/use-case/schema.json",
      "description": "Create Use Case Files for Domain Module"
    },
    "generate-json-schema": {
      "factory": "./src/schematics/library/use-case/schematic",
      "schema": "./src/schematics/library/use-case/schema.json",
      "description": "Create Use Case Files for Domain Module"
    }
  }
}
