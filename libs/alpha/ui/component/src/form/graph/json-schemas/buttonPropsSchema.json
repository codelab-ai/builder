{
  "type": "object",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "ButtonType": {
      "enum": [
        "dashed",
        "default",
        "ghost",
        "link",
        "primary",
        "text"
      ],
      "type": "string"
    },
    "ButtonShape": {
      "enum": [
        "circle",
        "round"
      ],
      "type": "string"
    },
    "SizeType": {
      "enum": [
        "large",
        "middle",
        "small"
      ],
      "type": "string"
    }
  },
  "properties": {
    "type": {
      "$ref": "#/definitions/ButtonType"
    },
    "shape": {
      "$ref": "#/definitions/ButtonShape"
    },
    "size": {
      "$ref": "#/definitions/SizeType"
    },
    "loading": {
      "anyOf": [
        {
          "type": "object",
          "properties": {
            "delay": {
              "type": "number"
            }
          }
        },
        {
          "type": "boolean"
        }
      ]
    },
    "prefixCls": {
      "type": "string"
    },
    "className": {
      "type": "string"
    },
    "ghost": {
      "type": "boolean"
    },
    "danger": {
      "type": "boolean"
    },
    "block": {
      "type": "boolean"
    }
  }
}