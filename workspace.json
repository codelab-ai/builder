{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/next"
  },
  "defaultProject": "web",
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/react": {
      "application": {
        "style": "css",
        "linter": "eslint",
        "babel": true
      },
      "library": {
        "style": "css",
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      },
      "component": {
        "style": "css"
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "@emotion/styled",
        "linter": "eslint"
      }
    },
    "@nrwl/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/nx-plugin": {
      "plugin": {
        "linter": "eslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/gatsby": {
      "application": {
        "style": "@emotion/styled"
      }
    }
  },
  "projects": {
    "web": {
      "root": "apps/web",
      "sourceRoot": "apps/web/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build-next": {
          "builder": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/web",
            "outputPath": "dist/apps/web",
            "assets": [
              {
                "glob": "pm2.json",
                "input": "apps/web",
                "output": "."
              }
            ]
          },
          "configurations": {
            "production": {}
          }
        },
        "build": {
          "builder": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/web",
            "outputPath": "dist/apps/web"
          },
          "configurations": {
            "production": {}
          }
        },
        "build-prod": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "yarn cross-env NODE_ENV=production nx build-next web --prod",
              "yarn copyfiles -f apps/web/pm2.json dist/apps/web"
            ],
            "parallel": false
          }
        },
        "serve": {
          "builder": "@nrwl/next:server",
          "options": {
            "buildTarget": "web:build-next",
            "dev": true,
            "port": 3000,
            "maxWorkers": 4,
            "memoryLimit": 2048
          },
          "configurations": {
            "ci": {
              "port": 3001
            },
            "production": {
              "buildTarget": "web:build-next:production",
              "dev": false
            }
          }
        },
        "export": {
          "builder": "@nrwl/next:export",
          "options": {
            "buildTarget": "web:build:production"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/web/**/*.ts",
              "apps/web/**/*.tsx",
              "apps/web/next-env.d.ts",
              "apps/web/**/*.spec.ts",
              "apps/web/**/*.spec.tsx",
              "apps/web/**/*.spec.js",
              "apps/web/**/*.spec.jsx",
              "apps/web/**/*.d.ts",
              "apps/src/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/web/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/apps/web"]
        },
        "storybook": {
          "builder": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": {
              "configFolder": "apps/web/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "builder": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/react",
            "outputPath": "dist/storybook/web",
            "config": {
              "configFolder": "apps/web/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
