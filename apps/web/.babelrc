{
  "presets": [
    "@babel/preset-react",
    "@babel/preset-typescript",
    "@babel/preset-env",
    // Don't use `@nrwl/next/babel`, has preset merging issues
    "next/babel"
    // "@nrwl/next/babel"
    // [
    //   "@nrwl/next/babel",
    //   {
    //     "useBuiltIns": "usage",
    //     "corejs": 3
    //   }
    // ]
  ],
  "plugins": [
    ["relay", {}],
    [
      "import",
      {
        "libraryName": "antd",
        "style": false
      }
    ],
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    "@babel/plugin-proposal-nullish-coalescing-operator"
  ]
}
