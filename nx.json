    "alpha-core-tree": { "tags": [] },
    "alpha-shared-factory": { "tags": [] },
    "alpha-shared-utils": { "tags": [] },
    "alpha-shared-interface-collections": { "tags": [] },
    "alpha-shared-interface-entity": { "tags": [] },
    "alpha-shared-interface-form": { "tags": [] },
    "alpha-shared-interface-graph": { "tags": [] },
    "alpha-shared-interface-graph-v2": { "tags": [] },
    "alpha-shared-interface-mapper": { "tags": [] },
    "alpha-shared-interface-node": { "tags": [] },
    "alpha-shared-interface-props": { "tags": [] },
    "alpha-shared-interface-tree": { "tags": [] },
    "alpha-ui-component": { "tags": [] },
    "alpha-ui-antd": { "tags": [] },
    "alpha-ui-d3": { "tags": [] },
